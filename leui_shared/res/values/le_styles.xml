<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <add-resource name="Widget.Leui" type="style" />

    <style name="Widget.Leui" parent="@android:Widget.DeviceDefault.Light"></style>

    <!-- To Make SourceHanSans font as default theme text font -->
    <add-resource name="Widget.Leui.TextView" type="style" />

    <style name="Widget.Leui.TextView" parent="@android:Widget.DeviceDefault.Light.TextView">
        <item name="android:includeFontPadding">false</item>
    </style>

    <add-resource name="Widget.Leui.EditText" type="style" />

    <style name="Widget.Leui.EditText" parent="@android:Widget.DeviceDefault.Light.EditText">
        <item name="android:includeFontPadding">false</item>
    </style>

    <add-resource name="Widget.Leui.EditText.Multiline" type="style" />

    <style name="Widget.Leui.EditText.Multiline">
        <item name="android:padding">10dp</item>
        <item name="android:lineSpacingMultiplier">1.6</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColorHint">#daddde</item>
        <item name="android:textColor">#38454f</item>
        <item name="android:background">@drawable/le_shape_edittext_multiline</item>
    </style>

    <add-resource name="Widget.Leui.LeListView" type="style" />

    <style name="Widget.Leui.LeListView">
        <item name="leSwipeMode">both</item>
        <item name="leSwipeActionLeft">reveal</item>
        <item name="leSwipeActionRight">reveal</item>
        <item name="leSwipeOffsetLeft">0dip</item>
        <item name="leSwipeOffsetRight">0dip</item>
        <item name="leSwipeOpenOnLongPress">true</item>
        <item name="leSwipeAnimationTime">0</item>
        <item name="leSwipeCloseAllItemsWhenMoveList">true</item>
        <item name="leSortDragScrollStart">0.33</item>
        <item name="leSortFloatAlpha">0.6</item>
        <item name="leSortMaxDragScrollSpeed">0.5</item>
        <item name="leSortSlideShuffleSpeed">0.3</item>
        <item name="leSortUseDefaultController">true</item>
        <item name="leSortEnabled">true</item>
    </style>

    <add-resource name="Widget.Leui.StaggeredGridView" type="style" />

    <style name="Widget.Leui.StaggeredGridView">
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:scrollbars">vertical</item>
    </style>

    <add-resource name="Widget.Leui.ListView.DropDown" type="style" />

    <style name="Widget.Leui.ListView.DropDown" parent="@android:Widget.DeviceDefault.Light.ListView.DropDown">
        <item name="android:divider">@drawable/le_list_dropdown_divider</item>
        <item name="android:scrollbars">none</item>
